{
    "manifest_version": 2,
    "name": "NTExt",
    "description": "customize new tab background image. (!important: image size should be < 5MB)",
    "version": "1.1.1",
    "icons":{
        "128":"img/218icon.png",
        "48":"img/48icon.png",
        "16":"img/16icon.png"
    },
    "browser_action": {
        "default_icon": {                
          "128": "img/218icon2.png",     
          "48": "img/48icon2.png",     
          "16": "img/16icon2.png"      
        },
        "default_title": "NTExt",  
        "default_popup": "popup.html"    
    },
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "options_page":"settings.html"
}